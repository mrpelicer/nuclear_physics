// Workspace settings
{
  // The following will hide the js and map files in the editor
  "files.exclude": {
      "**/*.png": true,
      "**/*.pdf": true,
      "**/*.avi": true,
      "**/*.zip": true,
      "**/*.txt": true
  },

  "files.watcherExclude": {
    "**/data/**": true,
    "**/data_save/**": true
}

}
